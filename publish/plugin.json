{
  "pluginName": "超级粘贴",
  "description": "将剪切板中的内容保存为文件",
  "author": "Ziu",
  "main": "index.html",
  "preload": "preload.js",
  "logo": "logo.png",
  "platform": ["win32", "darwin", "linux"],
  "version": "1.0.5",
  "features": [
    {
      "code": "监听模式",
      "explain": "切换监听模式开关",
      "cmds": ["监听模式"]
    },
    {
      "code": "超级粘贴设置",
      "explain": "修改插件设置",
      "cmds": ["超级粘贴设置", "设置"]
    },
    {
      "code": "收集文件",
      "explain": "保存到指定路径",
      "cmds": [
        {
          "type": "img",
          "label": "收集文件"
        },
        {
          "type": "over",
          "label": "收集文件"
        },
        {
          "type": "files",
          "label": "收集文件",
          "fileType": "file",
          "minLength": 1
        }
      ]
    }
  ]
}
